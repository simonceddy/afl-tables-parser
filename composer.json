{
    "name": "simoneddy/afl-tables-parser",
    "description": "Utilities for creating serializable objects from AFL Tables data",
    "type": "library",
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AflParser\\": "src"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/browser-kit": "^4.2",
        "ramsey/uuid": "^3.8",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0",
        "symfony/var-dumper": "^4.2",
        "illuminate/database": "^8.0",
        "illuminate/events": "^8.0"
    }
}
